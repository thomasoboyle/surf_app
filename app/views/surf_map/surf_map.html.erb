<!DOCTYPE html>
<html>
  <head>
    <style>
       .map {
        height: 600px;
        width: 100%;
       }
    </style>
  </head>
  <body>
    <h3>My Google Maps Demo</h3>
    <div class="map" id="mapUs"></div>
    <script>
      function initMap() {
        var hammonds = {lat: 34.415443, lng: -119.637207};
        var leadbetter = {lat: 34.398215, lng: -119.702134};
        var rincon = {lat: 34.373054, lng: -119.477274};
        // creates and centers map
        var mapUs = new google.maps.Map(document.getElementById('mapUs'), {
          zoom: 10,
          center: hammonds
        });
        
        var markerHammonds = new google.maps.Marker({
          position: hammonds,
          map: mapUs
        });

        var markerLeadbetter = new google.maps.Marker({
          position: leadbetter,
          map: mapUs
        });

        var markerRincon = new google.maps.Marker({
          position: rincon,
          map: mapUs
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDjg5tk0hGz6vI_aSnq_5U4iw7Qg2z5cjg&callback=initMap">
    </script>
  </body>
</html>
